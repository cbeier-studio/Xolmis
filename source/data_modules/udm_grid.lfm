    BeforePost = qImagesBeforePost
    Database = DMM.sqlCon
    Transaction = DMM.sqlTrans
    SQL.Strings = (
      'SELECT img.*,'
      '  z.full_name AS taxon_name,'
      '  i.full_name AS individual_name,'
      '  c.full_name AS capture_name,'
      '  g.site_name AS locality_name,'
      '  p.full_name AS author_name,'
      '  sv.full_name AS survey_name,'
      '  s.full_name AS sighting_name,'
      '  n.full_name AS nest_name,'
      '  nr.full_name AS revision_name,'
      '  e.full_name AS egg_name,'
      '  sp.full_name AS specimen_name'
      'FROM images AS img'
      'LEFT JOIN zoo_taxa AS z ON img.taxon_id = z.taxon_id'
      'LEFT JOIN individuals AS i ON img.individual_id = i.individual_id'
      'LEFT JOIN captures AS c ON img.capture_id = c.capture_id'
      'LEFT JOIN gazetteer AS g ON img.locality_id = g.site_id'
      'LEFT JOIN people AS p ON img.author_id = p.person_id'
      'LEFT JOIN surveys AS sv ON img.survey_id = sv.survey_id'
      'LEFT JOIN sightings AS s ON img.sighting_id = s.sighting_id'
      'LEFT JOIN nests AS n ON img.nest_id = n.nest_id'
      'LEFT JOIN nest_revisions AS nr ON img.nest_revision_id = nr.nest_revision_id'
      'LEFT JOIN eggs AS e ON img.egg_id = e.egg_id'
      'LEFT JOIN specimens AS sp ON img.specimen_id = sp.specimen_id'
      'WHERE (img.active_status = 1) AND (img.individual_id > 0)'
      'ORDER BY img.image_date, img.image_time ASC'
    )
