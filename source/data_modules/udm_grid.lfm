    SQL.Strings = (
      'SELECT bh.*,'
      '  it.acronym AS supplier_name,'
      '  p1.full_name AS sender_name,'
      '  p2.full_name AS requester_name'
      'FROM band_history AS bh'
      'LEFT JOIN institutions AS it ON bh.supplier_id = it.institution_id'
      'LEFT JOIN people AS p1 ON bh.sender_id = p1.person_id'
      'LEFT JOIN people AS p2 ON bh.requester_id = p2.person_id'
      'WHERE (bh.active_status = 1) AND (bh.band_id = :band_id)'
      'ORDER BY bh.event_date ASC'
    )
