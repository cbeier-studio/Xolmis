    SQL.Strings = (
      'SELECT m.*,'
      '  z.full_name AS taxon_name,'
      '  z.order_id AS order_id,'
      '  z.family_id AS family_id,'
      '  z.genus_id AS genus_id,'
      '  z.species_id AS species_id,'
      '  i.full_name AS individual_name,'
      '  p.acronym AS bander_name,'
      '  sv.full_name AS survey_name,'
      '  (b.band_size||'' ''||b.band_number) AS band_name'
      'FROM molts AS m'
      'LEFT JOIN zoo_taxa AS z ON m.taxon_id = z.taxon_id'
      'LEFT JOIN individuals AS i ON m.individual_id = i.individual_id'
      'LEFT JOIN people AS p ON m.bander_id = p.person_id'
      'LEFT JOIN surveys AS sv ON m.survey_id = sv.survey_id'
      'LEFT JOIN bands AS b ON m.band_id = b.band_id'
      'WHERE (m.individual_id = :individual_id)'
      '  AND (m.active_status = 1)'
      ''
    )
