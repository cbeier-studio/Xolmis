    SQL.Strings = (
      'SELECT i.*,'
      '(SELECT t.full_name FROM zoo_taxa AS t WHERE t.taxon_id = i.taxon_id) AS taxon_name,'
      '(SELECT d.full_name FROM individuals AS d WHERE d.individual_id = i.individual_id) AS individual_name,'
      '(SELECT m.full_name FROM captures AS m WHERE m.capture_id = i.capture_id) AS capture_name,'
      '(SELECT g.site_name FROM gazetteer AS g WHERE g.site_id = i.locality_id) AS locality_name,'
      '(SELECT p.full_name FROM people AS p WHERE p.person_id = i.author_id) AS author_name,'
      '(SELECT a.full_name FROM surveys AS a WHERE a.survey_id = i.survey_id) AS survey_name,'
      '(SELECT o.full_name FROM sightings AS o WHERE o.sighting_id = i.sighting_id) AS sighting_name,'
      '(SELECT n.full_name FROM nests AS n WHERE n.nest_id = i.nest_id) AS nest_name,'
      '(SELECT r.full_name FROM nest_revisions AS r WHERE r.nest_revision_id = i.nest_revision_id) AS revision_name,'
      '(SELECT e.full_name FROM eggs AS e WHERE e.egg_id = i.egg_id) AS egg_name,'
      '(SELECT s.full_name FROM specimens AS s WHERE s.specimen_id = i.specimen_id) AS specimen_name'
      'FROM images AS i'
      'WHERE (i.individual_id = :individual_id) AND (i.active_status = 1)'
      'ORDER BY i.image_date, i.image_time ASC'
    )
