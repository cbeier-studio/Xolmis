    Database = DMM.sqlCon
    Transaction = DMM.sqlTrans
    SQL.Strings = (
      'SELECT n.*,'
      '  (SELECT p.full_name FROM people AS p'
      '    WHERE p.person_id = n.observer_id) AS observer_name,'
      '  (SELECT j.project_title FROM projects AS j'
      '    WHERE j.project_id = n.project_id) AS project_name,'
      '  (SELECT g.site_name FROM gazetteer AS g'
      '    WHERE g.site_id = n.locality_id) AS locality_name,'
      '  (SELECT t.full_name FROM zoo_taxa AS t'
      '    WHERE t.taxon_id = n.taxon_id) AS taxon_name,'
      '  (SELECT t.formatted_name FROM zoo_taxa AS t'
      '    WHERE t.taxon_id = n.taxon_id) AS taxon_formatted_name,'
      '  (SELECT b.taxon_name FROM botanic_taxa AS b'
      '    WHERE b.taxon_id = n.support_plant_1_id) AS support_plant_1_name,'
      '  (SELECT b2.taxon_name FROM botanic_taxa AS b2'
      '    WHERE b2.taxon_id = n.support_plant_2_id) AS support_plant_2_name'
      'FROM nests AS n'
      'WHERE (n.nest_id IN (SELECT DISTINCT ow.nest_id FROM nest_owners AS ow'
      '                      WHERE (ow.individual_id = :individual_id) AND (ow.active_status = 1)))'
      '  AND (n.active_status = 1)'
    )
    Options = [sqoAutoApplyUpdates]
    Params = <    
      item
        DataType = ftUnknown
        Name = 'individual_id'
        ParamType = ptInput
      end>
