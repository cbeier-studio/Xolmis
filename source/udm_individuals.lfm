    SQL.Strings = (
      'SELECT m.*,'
      '  (SELECT z.full_name FROM zoo_taxa AS z'
      '    WHERE z.taxon_id = m.taxon_id) AS taxon_name,'
      '  (SELECT i.full_name FROM individuals AS i'
      '    WHERE i.individual_id = m.individual_id) AS individual_name,'
      '  (SELECT p.acronym FROM people AS p'
      '    WHERE p.person_id = m.bander_id) AS bander_name,'
      '  (SELECT a.full_name FROM surveys AS a'
      '    WHERE a.survey_id = m.survey_id) AS survey_name,'
      '  (SELECT b.band_size||'' ''||b.band_number FROM bands AS b'
      '    WHERE b.band_id = m.band_id) AS band_name'
      'FROM molts AS m'
      'WHERE (m.individual_id = :individual_id)'
      '  AND (m.active_status = 1)'
