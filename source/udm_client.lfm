    SQL.Strings = (
      'WITH MonthlyCaptures AS ('
      '    SELECT'
      '        STRFTIME(''%Y-%m'', capture_date) AS record_month,'
      '        COUNT(capture_id) AS quantity'
      '    FROM'
      '        captures'
      '    WHERE'
      '        capture_date > DATE(''now'', ''-12 months'')'
      '    GROUP BY'
      '        record_month'
      ')'
      'SELECT'
      '    ROW_NUMBER() OVER (ORDER BY record_month) AS id,'
      '    record_month,'
      '    quantity'
      'FROM'
      '    MonthlyCaptures'
      'ORDER BY'
      '    record_month ASC'
    )
