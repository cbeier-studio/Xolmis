    Transaction = DMM.sqlTrans
    SQL.Strings = (
      'SELECT sv.*,'
      '  (SELECT x.expedition_name FROM expeditions AS x'
      '    WHERE x.expedition_id = sv.expedition_id) AS expedition_name,'
      '  (SELECT gm.site_name FROM gazetteer AS gm'
      '    WHERE gm.site_id = sv.municipality_id) AS municipality_name,'
      '  (SELECT gs.site_name FROM gazetteer AS gs'
      '    WHERE gs.site_id = sv.state_id) AS state_name,'
      '  (SELECT gc.site_name FROM gazetteer AS gc'
      '    WHERE gc.site_id = sv.country_id) AS country_name,'
      '  (SELECT gl.full_name FROM gazetteer AS gl'
      '    WHERE gl.site_id = sv.locality_id) AS locality_name,'
      '  (SELECT ns.station_name FROM net_stations AS ns'
      '    WHERE ns.net_station_id = sv.net_station_id) AS net_station_name,'
      '  (SELECT mt.method_name FROM methods AS mt'
      '    WHERE mt.method_id = sv.method_id) AS method_name,'
      '  (SELECT pj.short_title FROM projects AS pj'
      '    WHERE pj.project_id = sv.project_id) AS project_name,'
      '  (SELECT cast((sum(ef.net_area) + sum(ef.open_time_total)) AS REAL) FROM nets_effort AS ef'
      '    WHERE (ef.survey_id = sv.survey_id) AND (ef.active_status = 1)) AS net_effort'
      'FROM surveys AS sv'
      'WHERE (sv.active_status = 1) AND (sv.expedition_id = :expedition_id)'
    )
