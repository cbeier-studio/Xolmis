      '  sv.full_name AS survey_name'
      'FROM nets_effort AS ef'
      'LEFT JOIN permanent_nets AS pn ON ef.permanent_net_id = pn.permanent_net_id'
      'LEFT JOIN surveys AS sv ON ef.survey_id = sv.survey_id'
      'WHERE (ef.survey_id = :survey_id)'
