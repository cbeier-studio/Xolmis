    Database = DMM.sqlCon
    Transaction = DMM.sqlTrans
    SQL.Strings = (
      'SELECT sv.*,'
      '  x.expedition_name AS expedition_name,'
      '  gl.full_name AS locality_name,'
      '  gm.site_name AS municipality_name,'
      '  gs.site_name AS state_name,'
      '  gc.site_name AS country_name,'
      '  ns.station_name AS net_station_name,'
      '  mt.method_name AS method_name,'
      '  pj.short_title AS project_name,'
      '  (SELECT CAST((SUM(ef.net_area) + SUM(ef.open_time_total)) AS REAL) FROM nets_effort AS ef'
      '    WHERE (ef.survey_id = sv.survey_id) AND (ef.active_status = 1)) AS net_effort'
      'FROM surveys AS sv'
      'LEFT JOIN expeditions AS x ON sv.expedition_id = x.expedition_id'
      'LEFT JOIN gazetteer AS gl ON sv.locality_id = gl.site_id'
      'LEFT JOIN gazetteer AS gm ON sv.municipality_id = gm.site_id'
      'LEFT JOIN gazetteer AS gs ON sv.state_id = gs.site_id'
      'LEFT JOIN gazetteer AS gc ON sv.country_id = gc.site_id'
      'LEFT JOIN net_stations AS ns ON sv.net_station_id = ns.net_station_id'
      'LEFT JOIN methods AS mt ON sv.method_id = mt.method_id'
      'LEFT JOIN projects AS pj ON sv.project_id = pj.project_id'
      'WHERE (sv.active_status = 1) AND (sv.expedition_id = :expedition_id)'
    )
