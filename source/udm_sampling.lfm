    SQL.Strings = (
      'SELECT c.*,'
      '  (SELECT t.full_name FROM zoo_taxa AS t'
      '    WHERE t.taxon_id = c.taxon_id) AS taxon_name,'
      '  (SELECT h.formatted_name FROM zoo_taxa AS h'
      '    WHERE h.taxon_id = c.taxon_id) AS taxon_formatted_name,'
      '  (SELECT i.full_name FROM individuals AS i'
      '    WHERE i.individual_id = c.individual_id) AS individual_name,'
      '  (SELECT p.full_name FROM people AS p'
      '    WHERE p.person_id = c.observer_id) AS observer_name,'
      '  (SELECT a.full_name FROM surveys AS a'
      '    WHERE a.survey_id = c.survey_id) AS survey_name,'
      '  (SELECT m.method_name FROM methods AS m'
      '    WHERE m.method_id = c.method_id) AS method_name,'
      '  (SELECT g.full_name FROM gazetteer AS g'
      '    WHERE g.site_id = c.locality_id) AS locality_name'
      'FROM sightings AS c'
      'WHERE (c.survey_id = :survey_id)'
      '  AND (c.active_status = 1)'
    )
    Options = [sqoAutoApplyUpdates]
