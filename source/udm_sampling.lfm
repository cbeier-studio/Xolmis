    Transaction = DMM.sqlTrans
    SQL.Strings = (
      'SELECT i.*,'
      '(SELECT t.full_name FROM zoo_taxa AS t WHERE t.taxon_id = i.taxon_id) AS taxon_name,'
      '(SELECT d.full_name FROM individuals AS d WHERE d.individual_id = i.individual_id) AS individual_name,'
      '(SELECT m.full_name FROM captures AS m WHERE m.capture_id = i.capture_id) AS capture_name,'
      '(SELECT g.site_name FROM gazetteer AS g WHERE g.site_id = i.locality_id) AS locality_name,'
      '(SELECT p.full_name FROM people AS p WHERE p.person_id = i.author_id) AS author_name,'
      '(SELECT a.full_name FROM surveys AS a WHERE a.survey_id = i.survey_id) AS survey_name,'
      '(SELECT o.full_name FROM sightings AS o WHERE o.sighting_id = i.sighting_id) AS sighting_name,'
      '(SELECT n.full_name FROM nests AS n WHERE n.nest_id = i.nest_id) AS nest_name,'
      '(SELECT r.full_name FROM nest_revisions AS r WHERE r.nest_revision_id = i.nest_revision_id) AS revision_name,'
      '(SELECT e.full_name FROM eggs AS e WHERE e.egg_id = i.egg_id) AS egg_name,'
      '(SELECT s.full_name FROM specimens AS s WHERE s.specimen_id = i.specimen_id) AS specimen_name'
      'FROM images AS i'
      'WHERE (i.survey_id = :survey_id) AND (i.active_status = 1)'
      'ORDER BY i.image_date, i.image_time ASC'
    )
    UpdateSQL.Strings = (
      'update  set "marked_status"=:"marked_status","image_thumbnail"=:"image_thumbnail","image_date"=:"image_date","image_time"=:"image_time","image_type"=:"image_type","author_id"=:"author_id","taxon_id"=:"taxon_id","subtitle"=:"subtitle","locality_id"=:"locality_id","coordinate_precision"=:"coordinate_precision","longitude"=:"longitude","latitude"=:"latitude","individual_id"=:"individual_id","capture_id"=:"capture_id","survey_id"=:"survey_id","sighting_id"=:"sighting_id","nest_id"=:"nest_id","nest_revision_id"=:"nest_revision_id","egg_id"=:"egg_id","specimen_id"=:"specimen_id","license_type"=:"license_type","license_year"=:"license_year","license_uri"=:"license_uri","license_notes"=:"license_notes","license_owner"=:"license_owner","image_filename"=:"image_filename","order_id"=:"order_id","family_id"=:"family_id","genus_id"=:"genus_id","species_id"=:"species_id","country_id"=:"country_id","state_id"=:"state_id","municipality_id"=:"municipality_id","user_inserted"=:"user_inserted","user_updated"=:"user_updated","insert_date"=:"insert_date","update_date"=:"update_date","exported_status"=:"exported_status","active_status"=:"active_status" where ("image_id"= :"OLD_image_id") '
