    SQL.Strings = (
      'SELECT m.*,'
      '  (SELECT t.full_name FROM zoo_taxa AS t'
      '    WHERE t.taxon_id = m.taxon_id) AS taxon_name,'
      '  (SELECT h.formatted_name FROM zoo_taxa AS h'
      '    WHERE h.taxon_id = m.taxon_id) AS taxon_formatted_name,'
      '  (SELECT s.full_name FROM surveys AS s'
      '    WHERE s.survey_id = m.survey_id) AS survey_name,'
      '  (SELECT ns.station_name FROM net_stations AS ns'
      '    WHERE ns.net_station_id = m.net_station_id) AS net_station_name,'
      '  (SELECT n.net_number FROM nets_effort AS n'
      '    WHERE n.net_id = m.net_id) AS net_number,'
      '  (SELECT g.site_name FROM gazetteer As g'
      '    WHERE g.site_id = m.locality_id) AS locality_name,'
      '  (SELECT b.acronym FROM people AS b'
      '    WHERE b.person_id = m.bander_id) AS bander_name,'
      '  (SELECT w.acronym FROM people AS w'
      '    WHERE w.person_id = m.annotator_id) AS annotator_name,'
      '  (SELECT (a.band_size||'' ''||a.band_number) FROM bands AS a'
      '    WHERE a.band_id = m.band_id) AS band_name,'
      '  (SELECT (r.band_size||'' ''||r.band_number) FROM bands AS r'
      '    WHERE r.band_id = m.removed_band_id) AS removed_band_name,'
      '  (SELECT f.acronym FROM people AS f'
      '    WHERE f.person_id = m.photographer_1_id) AS photographer_1_name,'
      '  (SELECT f2.acronym FROM people AS f2'
      '    WHERE f2.person_id = m.photographer_2_id) AS photographer_2_name'
      'FROM captures AS m'
      'WHERE (m.survey_id = :survey_id)'
      '  AND (m.active_status = 1)'
      ''
    )
    Options = [sqoAutoApplyUpdates]
